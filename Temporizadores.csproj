<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RazorLangVersion>3.0</RazorLangVersion>
  </PropertyGroup>
  
  <PropertyGroup>
    <BlazorEnableCompression>false</BlazorEnableCompression>
  </PropertyGroup>
  
  <ItemGroup>
    <None Remove="docs\service-worker.js" />
    <None Remove="docs\wwwroot\.nojekyll" />
    <None Remove="docs\wwwroot\404.html" />
    <None Remove="docs\wwwroot\css\app.css" />
    <None Remove="docs\wwwroot\css\bootstrap\bootstrap.min.css" />
    <None Remove="docs\wwwroot\css\bootstrap\bootstrap.min.css.map" />
    <None Remove="docs\wwwroot\css\open-iconic\FONT-LICENSE" />
    <None Remove="docs\wwwroot\css\open-iconic\font\css\open-iconic-bootstrap.min.css" />
    <None Remove="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.eot" />
    <None Remove="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.otf" />
    <None Remove="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.svg" />
    <None Remove="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.ttf" />
    <None Remove="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.woff" />
    <None Remove="docs\wwwroot\css\open-iconic\ICON-LICENSE" />
    <None Remove="docs\wwwroot\css\open-iconic\README.md" />
    <None Remove="docs\wwwroot\Data\beep.mp3" />
    <None Remove="docs\wwwroot\favicon.ico" />
    <None Remove="docs\wwwroot\icon-512.png" />
    <None Remove="docs\wwwroot\index.html" />
  </ItemGroup>
  
  <ItemGroup>
    <Content Include="docs\service-worker.js">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\.nojekyll">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\404.html">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\app.css">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\bootstrap\bootstrap.min.css">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\bootstrap\bootstrap.min.css.map">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\open-iconic\FONT-LICENSE">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\open-iconic\font\css\open-iconic-bootstrap.min.css">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.eot">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.otf">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.svg">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Include="docs\wwwroot\css\open-iconic\font\fonts\open-iconic.ttf">
